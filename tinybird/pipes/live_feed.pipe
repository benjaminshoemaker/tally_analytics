NODE live_events
SQL >
    %
    SELECT
        project_id,
        event_type,
        path,
        referrer,
        timestamp,
        formatReadableTimeDelta(now() - timestamp) as relative_time
    FROM events
    WHERE project_id = {{String(project_id, '')}}
    AND timestamp > {{DateTime(since, '2024-01-01 00:00:00')}}
    ORDER BY timestamp DESC
    LIMIT {{Int32(limit, 20)}}
